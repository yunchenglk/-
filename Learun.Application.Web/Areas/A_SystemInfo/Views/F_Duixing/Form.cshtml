@{
    ViewBag.Title = "业务对象信息字典管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap">
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">对象编码<font face="宋体">*</font></div>
        <input id="BM" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">对象名称<font face="宋体">*</font></div>
        <input id="NAME" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">输入码</div>
        <input id="SRM" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">对象类别</div>
        <div id="CATA"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">是否有效</div>
        <div id="F_EnabledMark"></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">是否删除</div>
        <div id="F_DeleteMark"></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">所属公司</div>
        <input id="F_CompanyId" type="text" readonly class="form-control currentInfo lr-currentInfo-company" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">创建时间</div>
        <input id="F_CreateDate" type="text" readonly class="form-control currentInfo lr-currentInfo-user" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Duixiang" >
        <div class="lr-form-item-title">创建人</div>
        <input id="F_CreateUserId" type="text" readonly class="form-control currentInfo lr-currentInfo-user" />
    </div>
</div>
@Html.AppendJsFile("/Areas/A_SystemInfo/Views/F_Duixing/Form.js")
