@{
    ViewBag.Title = "药品科目信息字典管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap">
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">科目编码<font face="宋体">*</font></div>
        <textarea id="BM" class="form-control" style="height:100px;" isvalid="yes" checkexpession="NotNull" ></textarea>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">科目名称<font face="宋体">*</font></div>
        <textarea id="NAME" class="form-control" style="height:100px;" isvalid="yes" checkexpession="NotNull" ></textarea>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">加成比例</div>
        <textarea id="JCBL" class="form-control" style="height:100px;"  ></textarea>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">输入码<font face="宋体">*</font></div>
        <textarea id="SRM" class="form-control" style="height:100px;" isvalid="yes" checkexpession="NotNull" ></textarea>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">是否有效</div>
        <div id="F_EnabledMark"></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">是否删除</div>
        <div id="F_DeleteMark"></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">创建人</div>
        <input id="F_CreateUserId" type="text" readonly class="form-control currentInfo lr-currentInfo-user" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">创建时间</div>
        <input id="F_CreateDate" type="text" readonly class="form-control currentInfo lr-currentInfo-time" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="A_Yaopin_Km" >
        <div class="lr-form-item-title">所属公司<font face="宋体">*</font></div>
        <input id="F_CompanyId" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
</div>
@Html.AppendJsFile("/Areas/A_SystemInfo/Views/A_Yaopin_Km/Form.js")
